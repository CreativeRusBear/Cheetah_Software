extends templates/head

block title
    title #{title}

block content
    include templates/header
    section
        div(class='os_article')
            div(class='info content')
                each val, prop in data
                    p
                        span(class='capitalize') #{prop}:
                        span #{val}
            img(src="img/articles/os/undraw_online_wishes_dlmr.svg" alt="os")